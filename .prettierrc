{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
		"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/hooks/",
		"/screens/",
		"/components/",
		"^../(.*)",
		"^./(.*)",
		"/module.scss/"
	],
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
